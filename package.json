{
  "name": "ts-extensions-priority",
  "displayName": "TS extensions priority",
  "description": "Allows to change TypeScript resolving extensions priority",
  "version": "0.1.1",
  "publisher": "id141365154",
  "categories": [
    "Language Packs",
    "Other"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/id141365154/vscode-type-resolver"
  },
  "icon": "./logo.png",
  "engines": {
    "vscode": "^1.42.0"
  },
  "keywords": [
    "Typescript",
    "Extension",
    "Custom extensions",
    "Monorepo tool"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extensionsPriority.quickInput",
        "title": "Extensions priority"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  }
}
